#unset -f pfd
unset -f pfs
#unset -f cdf
unset -f pushdf
unset -f pxd
unset -f cdx
unset -f quick-look
unset -f man-preview
unalias showfiles
unalias hidefiles
unset -f itunes
unset -f music
unset -f spotify
unset -f rmdsstore
unset -f btrestart
unset -f freespace

